<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@android:color/background_light</item>
        <item name="colorPrimaryDark">@android:color/background_light</item>
        <item name="colorAccent">@android:color/background_light</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/background_light</item>
    </style>

    <style name="da_background_password_button">
        <item name="android:background">@drawable/da_behaviour_btn_password</item>
    </style>

    <style name="da_background_fingerprint_button">
        <item name="android:background">@drawable/da_behaviour_btn_fingerprint</item>
    </style>

    <style name="da_background_password_item">
        <item name="android:background">@drawable/fingerprint_button_normal</item>
    </style>

</resources>
